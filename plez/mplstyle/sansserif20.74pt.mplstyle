# Basic rcParams for serif fonts that are all 20.74 pt

## FONT
font.family : sans-serif
font.size : 20.74
## AXES
axes.titlesize : 20.74
axes.labelsize : 20.74
## TICKS
xtick.labelsize : 20.74
ytick.labelsize : 20.74
xtick.direction : in
ytick.direction : in
## LEGEND
legend.fontsize : 20.74
## FIGURE
figure.titlesize : 20.74
figure.labelsize : 20.74
