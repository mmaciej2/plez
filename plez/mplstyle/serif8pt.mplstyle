# Basic rcParams for serif fonts that are all 8 pt

## FONT
font.family : serif
font.size : 8
## AXES
axes.titlesize : 8
axes.labelsize : 8
## TICKS
xtick.labelsize : 8
ytick.labelsize : 8
xtick.direction : in
ytick.direction : in
## LEGEND
legend.fontsize : 8
## FIGURE
figure.titlesize : 8
figure.labelsize : 8
