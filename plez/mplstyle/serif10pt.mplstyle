# Basic rcParams for serif fonts that are all 10 pt

## FONT
font.family : serif
font.size : 10
## AXES
axes.titlesize : 10
axes.labelsize : 10
## TICKS
xtick.labelsize : 10
ytick.labelsize : 10
xtick.direction : in
ytick.direction : in
## LEGEND
legend.fontsize : 10
## FIGURE
figure.titlesize : 10
figure.labelsize : 10
