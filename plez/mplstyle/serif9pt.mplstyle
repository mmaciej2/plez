# Basic rcParams for serif fonts that are all 9 pt

## FONT
font.family : serif
font.size : 9
## AXES
axes.titlesize : 9
axes.labelsize : 9
## TICKS
xtick.labelsize : 9
ytick.labelsize : 9
xtick.direction : in
ytick.direction : in
## LEGEND
legend.fontsize : 9
## FIGURE
figure.titlesize : 9
figure.labelsize : 9
